<%= form_for @caloriesburnd do |f| %>
  <div class="form-group">
    <%= f.label :メニュー名 %>
    <%= f.text_field :name %>
  </div>

  <div class="form-group">
    <%= f.label :mets %>
    <%= f.number_field :mets, step: "0.1" %>
  </div>

  <%= f.submit '投稿', class: 'btn btn-black' %>
<% end %>




  